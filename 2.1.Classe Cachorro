//classe cachorro

public class Cachorro {

    String nome;
    String raca;
    int idade;


    public Cachorro(String nome, String raca, int idade) {
        this.nome = nome;
        this.raca = raca;
        this.idade = idade;
    }


    public void latir() {
        System.out.println("Au Au!");
    }
}

//classe main

public class Main {
    public static void main(String[] args) {

        Cachorro meuCachorro = new Cachorro("Rex", "Pitt Bull", 2);

        meuCachorro.latir();
    }
}
